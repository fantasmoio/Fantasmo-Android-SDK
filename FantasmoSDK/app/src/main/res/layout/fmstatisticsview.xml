        app:layout_constraintTop_toTopOf="@+id/pitchLowText" />

    <TextView
        android:id="@+id/pitchHighText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/fmTransparentWhite"
        android:text="@string/pitch_high"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/pitchLowTextView"
