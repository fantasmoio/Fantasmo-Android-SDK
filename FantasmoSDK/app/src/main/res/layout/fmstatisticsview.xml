        app:layout_constraintBottom_toBottomOf="@+id/tooFastTextView"
        app:layout_constraintStart_toEndOf="@+id/tooFastTextView"
        app:layout_constraintTop_toTopOf="@+id/tooFastTextView" />

    <TextView
        android:id="@+id/tooLittleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/fmTransparentWhite"
        android:text="@string/_0"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tooLittleText"
        app:layout_constraintStart_toEndOf="@+id/tooLittleText"
        app:layout_constraintTop_toTopOf="@+id/tooLittleText" />

    <TextView
        android:id="@+id/featuresText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/fmTransparentWhite"
        android:text="@string/features"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tooLittleTextView"
